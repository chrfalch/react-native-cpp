{
  "name": "Example",
  "sources": [
    "cpp/*.h",
    "cpp/api/*"
  ],
  "ios": {
    "dependencies": [
      "RCTTypeSafety"
    ],
    "podspec": {
      "frameworks": [
        "MetalKit"
      ],
      "pod_target_xcconfig": {
        "GCC_PREPROCESSOR_DEFINITIONS": "$(inherited) SK_METAL=1 SK_GANESH=1",
        "CLANG_CXX_LANGUAGE_STANDARD": "c++17",
        "DEFINES_MODULE": "YES",
        "HEADER_SEARCH_PATHS": "\"$(PODS_TARGET_SRCROOT)/cpp/\"/**"
      }
    }
  },
  "android": {
    "dependencies": []
  }
}